UNITS LENGTH=A TIME=fs ENERGY=kcal/mol
#----Read colvars in-------
proj: READ FILE=../outputs/COLVAR_cat__FORCE_.dat  VALUES=proj IGNORE_FORCES IGNORE_TIME
ext: READ FILE=../outputs/COLVAR_cat__FORCE_.dat  VALUES=ext IGNORE_FORCES IGNORE_TIME
cmap: READ FILE=../outputs/CMAP_cat__FORCE_.dat  VALUES=cmap IGNORE_FORCES IGNORE_TIME
#----Read biases ------- 
wall: READ FILE=../outputs/WALL_cat__FORCE_.dat VALUES=totalwall.bias IGNORE_FORCES IGNORE_TIME
wham: READ FILE=../outputs/BIAS_cat__FORCE_.dat VALUES=wham.bias IGNORE_FORCES IGNORE_TIME

#----get weights-------
weights: REWEIGHT_BIAS ARG=wham.bias TEMP=300.0

#---Histogram with weights----
#----1D-----
projhist: HISTOGRAM ARG=proj STRIDE=1 GRID_MIN=_MINP_ GRID_MAX=_MAXP_ GRID_BIN=200 BANDWIDTH=0.06 LOGWEIGHTS=weights 
exthist: HISTOGRAM ARG=ext STRIDE=1 GRID_MIN=_MINE_ GRID_MAX=_MAXE_ GRID_BIN=200 BANDWIDTH=0.06 LOGWEIGHTS=weights 
cmaphist: HISTOGRAM ARG=cmap STRIDE=1 GRID_MIN=0.0 GRID_MAX=1 GRID_BIN=100 BANDWIDTH=0.04 LOGWEIGHTS=weights 

#----2D----
proj_ext: HISTOGRAM ARG=proj,ext STRIDE=1  GRID_MIN=_MINP_,_MINE_ GRID_MAX=_MAXP_,_MAXE_ GRID_BIN=100,100 BANDWIDTH=0.07,0.07 LOGWEIGHTS=weights 
proj_cmap: HISTOGRAM ARG=proj,cmap STRIDE=1 GRID_MIN=_MINP_,0 GRID_MAX=_MAXP_,1.0 GRID_BIN=100,100 BANDWIDTH=0.1,0.04 LOGWEIGHTS=weights
ext_cmap: HISTOGRAM ARG=ext,cmap STRIDE=1 GRID_MIN=_MINE_,0 GRID_MAX=_MAXE_,1.0 GRID_BIN=100,100 BANDWIDTH=0.1,0.04 LOGWEIGHTS=weights

#---Convert to FES----
feproj: CONVERT_TO_FES GRID=projhist TEMP=300 MINTOZERO
feext: CONVERT_TO_FES GRID=exthist TEMP=300 MINTOZERO
fecmap: CONVERT_TO_FES GRID=cmaphist TEMP=300 MINTOZERO

fftwocv: CONVERT_TO_FES GRID=proj_ext TEMP=300 MINTOZERO
ffprojcmap: CONVERT_TO_FES GRID=proj_cmap TEMP=300 MINTOZERO
ffextcmap: CONVERT_TO_FES GRID=ext_cmap TEMP=300 MINTOZERO

DUMPGRID GRID=feproj  FILE=../outputs/F_FORCE__fes_proj_rw.dat FMT=%.24e
DUMPGRID GRID=feext  FILE=../outputs/F_FORCE__fes_ext_rw.dat FMT=%.24e
DUMPGRID GRID=fecmap  FILE=../outputs/F_FORCE__fes_cmap_rw.dat FMT=%.24e

DUMPGRID GRID=fftwocv FILE=../outputs/F_FORCE__fes_projext_rw.dat FMT=%.24e
DUMPGRID GRID=ffprojcmap  FILE=../outputs/F_FORCE__fes_projcmap_rw.dat FMT=%.24e
DUMPGRID GRID=ffextcmap  FILE=../outputs/F_FORCE__fes_extcmap_rw.dat FMT=%.24e

PRINT ARG=proj,ext,cmap,wham.bias,weights STRIDE=1 FILE=../outputs/COLVAR_cat__FORCE__weights.out
